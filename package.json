    "node-record-lpcm16": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "eslint": "^8.57.1",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "prettier": "^3.4.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5"
